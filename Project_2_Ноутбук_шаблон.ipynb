      "source": [
        "# текст запроса\n",
        "query_6_5 = f'''\n",
        "                SELECT\n",
        "                    ROUND(AVG((LENGTH(key_skills) - LENGTH(REPLACE(key_skills, N'\\t', N'')) + 1)), 2) AS avg\n",
