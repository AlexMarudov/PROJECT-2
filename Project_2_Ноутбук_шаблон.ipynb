        "                    GROUP BY AREAS.id\n",
        "                )\n",
        "                UNION\n",
        "                (\n",
        "                    SELECT\n",
        "                        COUNT(*),\n",
        "                        'Total'\n",
        "                    FROM VACANCIES\n",
        "                    INNER JOIN EMPLOYERS ON EMPLOYERS.id = VACANCIES.employer_id\n",
